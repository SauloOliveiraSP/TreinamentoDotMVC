@model List<UserViewModel>

<div class="text-center">
    <h1>Boa Tarde</h1>
    @Html.ActionLink("Novo Usuario", "Index", "Home")
    <div class="table">
        <table border="1">
            <thead>
                <tr>
                    <th>Código</th>
                    <th>Pessoa</th>
                    <th>CPF/CNPJ</th>
                    <th>Fantasia</th>
                    <th>Nome</th>
                    <th>E-mail</th>
                    <th>Telefone</th>
                    <th>Cep</th>
                    <th>Endereço</th>
                    <th>Cidade</th>
                    <th>UF</th>
                    <th>País</th>
                    <th>Bairro</th>
                    <th>Fundação/Aniv</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var usuarios in Model)
                {
                <tr>
                    <td> @usuarios.Codigo</td>
                    <td> @usuarios.Pessoa</td>
                    <td> @usuarios.Cpf </td>
                    <td> @usuarios.Fantasia </td>
                    <td> @usuarios.Nome </td>
                    <td> @usuarios.Email</td>
                    <td> @usuarios.Telefone</td>
                    <td> @usuarios.Cep</td>
                    <td> @usuarios.Endereco</td>
                    <td> @usuarios.Cidade</td>
                    <td> @usuarios.Uf</td>
                    <td> @usuarios.Pais</td>
                    <td> @usuarios.Bairro</td>
                    <td> @usuarios.Fundacao</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
